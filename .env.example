# Billbotpoochuay Landing Environment Variables

# API Configuration
VITE_API_BASE_URL=http://localhost:8000
VITE_API_VERSION=v1

# Webview Configuration
VITE_WEBVIEW_MODE=true
VITE_SESSION_TIMEOUT=900000  # 15 minutes in milliseconds

# Facebook Messenger Extensions
VITE_MESSENGER_EXTENSIONS_VERSION=2.1
VITE_FACEBOOK_APP_ID=1256132279440475

# Security
VITE_ENABLE_CSP=true
VITE_ALLOWED_ORIGINS=https://messenger.com,https://www.messenger.com

# Features
VITE_ENABLE_ONBOARDING=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true

# Language
VITE_DEFAULT_LANGUAGE=th
VITE_SUPPORTED_LANGUAGES=th,en

# Development
VITE_DEBUG=false